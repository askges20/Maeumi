<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hanium.android.maeumi">
    <!-- Dialogflow 연결 -->
    <uses-permission android:name="android.permission.INTERNET" />

    <!--    앨범 접근 권한-->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/Theme.Maeumi">
        <activity android:name=".ActivitySplash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SingUpActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity"/>
        <activity android:name=".view.diary.DiaryContent" />
        <activity android:name=".view.diary.DiaryModify" />
        <activity android:name=".view.diary.DiaryWrite" />
        <activity android:name=".view.chatbot.ChatBot" />
        <activity android:name=".view.chatbot.ChatBotKeyWord" />
        <activity android:name=".view.chatbot.ChatBotKeyWordNO" />
        <activity android:name=".view.chatbot.ChatBotKeyWordYES" />
        <activity android:name=".view.board.Board" />
        <activity android:name=".view.board.BoardWrite" />
        <activity android:name=".view.board.PostContent" />
        <activity android:name=".view.board.PostModify" />
        <activity android:name=".view.profile.Profile" />
        <activity android:name=".view.profile.ProfileEdit" />
        <activity android:name=".view.selftest.SelfTest" />
        <activity android:name=".view.selftest.TestResult" />
        <activity android:name=".view.selftest.TestClick" />
        <activity android:name=".view.diary.DiaryMain" />



    </application>


</manifest>